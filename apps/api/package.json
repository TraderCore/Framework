{
    "name": "@tradercore/app-api",
    "version": "1.0.1",
    "description": "TraderCore Framework API - Backend service for the trading platform",
    "author": "TheGoatedDev",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/tradercore/framework.git",
        "directory": "apps/api"
    },
    "bugs": {
        "url": "https://github.com/tradercore/framework/issues"
    },
    "keywords": ["api", "trading", "backend", "nestjs", "tradercore"],
    "scripts": {
        "check-types": "tsc --noEmit",
