{
    "name": "@tradercore/plugin-manager",
    "version": "1.0.0",
    "type": "module",
    "imports": {
        "@/*": "./dist/*"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "check-types": "tsc --noEmit",
        "build": "tsc && tsc-alias",
        "dev": "tsc-watch --onSuccess 'tsc-alias'",
        "test": "jest --passWithNoTests"
    },
    "dependencies": {
        "@nestjs/common": "10.4.7",
        "@nestjs/config": "3.3.0",
        "@nestjs/core": "10.4.7",
        "@tradercore/core": "workspace:*"
    },
    "devDependencies": {
        "@tradercore/jest-config": "workspace:*",
        "@tradercore/typescript-config": "workspace:*",
        "@types/jest": "29.5.14",
        "@types/node": "22.9.0",
        "jest": "29.7.0",
        "typescript": "5.6.3",
        "tsc-alias": "1.8.10",
        "tsc-watch": "6.2.1"
    }
}
