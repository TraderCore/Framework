{
    "name": "@tradercore/plugin-template",
    "version": "1.0.0",
    "imports": {
        "@/*": "./dist/*"
    },
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "check-types": "tsc --noEmit",
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "jest --passWithNoTests"
    },
    "dependencies": {
        "@nestjs/common": "11.0.7",
        "@nestjs/config": "4.0.0",
        "@nestjs/core": "11.0.7",
        "@tradercore/framework": "workspace:*"
    },
    "devDependencies": {
        "@tradercore/jest-config": "workspace:*",
        "@tradercore/typescript-config": "workspace:*",
        "@types/jest": "29.5.14",
        "jest": "29.7.0",
        "tsup": "8.3.6",
        "typescript": "5.7.3"
    }
}
