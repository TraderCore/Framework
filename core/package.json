{
    "name": "@tradercore/core",
    "version": "1.0.0",
    "type": "module",
    "imports": {
        "@/*": "./dist/*"
    },
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "default": "./dist/index.js"
        },
        "./config": {
            "types": "./src/modules/config/index.ts",
            "default": "./dist/modules/config/index.js"
        }
    },
    "scripts": {
        "check-types": "tsc --noEmit",
        "build": "tsc && tsc-alias",
        "dev": "tsc-watch --onSuccess 'tsc-alias'",
        "test": "jest --passWithNoTests"
    },
    "dependencies": {
        "@nestjs/common": "10.4.7",
        "@nestjs/config": "3.3.0",
        "@nestjs/core": "10.4.7",
        "@nestjs/swagger": "8.0.7",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@tradercore/jest-config": "workspace:*",
        "@tradercore/typescript-config": "workspace:*",
        "@types/jest": "29.5.14",
        "jest": "29.7.0",
        "typescript": "5.6.3",
        "tsc-watch": "6.2.1",
        "tsc-alias": "1.8.10"
    }
}
